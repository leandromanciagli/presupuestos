             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence
             http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd"
             version="2.1">
    <persistence-unit name="presupuestos">
        <provider>org.hibernate.ejb.HibernatePersistence</provider>
        <class>com.presupuestos.presupuestos.entity.Item</class>
        <class>com.presupuestos.presupuestos.entity.Visitant</class>
        <properties>
            <!--            <property name="hibernate.default_schema" value="jyaa2024_bd10"/>-->
