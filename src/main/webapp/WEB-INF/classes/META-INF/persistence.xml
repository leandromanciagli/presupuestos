            <property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
            <property name="hibernate.connection.url" value="jdbc:mysql:mysql.java.linti.unlp.edu.ar/jyaa2024_bd10"/>
            <property name="hibernate.connection.username" value="jyaa2024_usr10"/>
            <property name="hibernate.connection.password" value="jyaa2024_pwd10"/>
        </properties>
    </persistence-unit>
</persistence>
